{
	"name": "@arcmantle/prospector",
	"version": "1.0.0",
	"description": "Automatic semantic versioning from git history for trunk-based development",
	"keywords": [
		"semver",
		"versioning",
		"git",
		"trunk-based",
		"semantic-version",
		"ci-cd",
		"github-action",
		"automation"
	],
	"homepage": "https://github.com/arcmantle/weave/tree/main/packages/dev-tools/prospector#readme",
	"bugs": {
		"url": "https://github.com/arcmantle/weave/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arcmantle/weave.git",
		"directory": "packages/dev-tools/prospector"
	},
	"license": "MIT",
	"author": "arcmantle",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"prospector": "./bin/bin.js"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"CHANGELOG.md",
		"action.yml"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsc --watch",
		"test": "vitest"
	},
	"dependencies": {
		"@actions/core": "catalog:",
		"ora": "catalog:",
		"semver": "catalog:"
	},
	"devDependencies": {
		"@types/node": "catalog:",
		"@types/semver": "catalog:",
		"tsdown": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	},
	"tsdown": [
		{
			"entry": [
				"src/index.ts",
				"src/cli.ts"
			],
			"platform": "node",
			"format": "esm",
			"outDir": "dist",
			"noExternal": [
				"ora",
				"semver"
			]
		},
		{
			"entry": [
				"src/action.ts"
			],
			"platform": "node",
			"format": "esm",
			"outDir": "action",
			"dts": false,
			"noExternal": [
				"@actions/core",
				"ora",
				"semver"
			]
		}
	]
}
